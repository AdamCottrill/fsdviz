{
  "name": "FishStockingDataViz",
  "version": "0.0.3",
  "repository": "https://github.com/AdamCottrill/fsdivz",
  "jest": {
    "testPathIgnorePatterns": [
      "js_src/tests/.#"
    ]
  },
  "description": "Great Lakes Fish Stocking Data Visualization",
  "main": "index.js",
  "scripts": {
    "dev": "rollup -c --watch --watch.include fsdviz/js_src/**",
    "rollup": "rollup -c",
    "build": "rollup -c --environment NODE_ENV:production",
    "reload": "live-server --port=8111",
    "watch": "npm-run-all --parallel reload dev",
    "test": "jest  --maxWorkers=4",
    "format": "prettier-eslint --eslint-config-path ./.eslintrc.js --write 'fsdviz/js_src/*.js'",
    "lint": "eslint fsdviz/js_src --fix --ext .js"
  },
  "author": "Adam Cottrill",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.10",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "concurrently": "^6.0.0",
    "debug": "^4.1.1",
    "eslint": ">=5.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^26.6.3",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "regenerator-runtime": "^0.13.7",
    "rollup": "^2.41.1",
    "rollup-plugin-env": "^0.21.4",
    "rollup-plugin-terser": "^7.0.2",
    "terser": "^5.6.0"
  },
  "dependencies": {
    "@terraformer/wkt": "^2.0.7",
    "@turf/bbox": "^6.0.1",
    "@turf/boolean-point-in-polygon": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@turf/turf": "^6.3.0",
    "crossfilter2": "^1.4.7",
    "d3": "5.16.0",
    "d3-collection": "^1.0.7",
    "d3-format": "^2.0.0",
    "d3-interpolate": "^2.0.1",
    "d3-scale": "^3.2.3",
    "d3-selection": "^2.0.0",
    "d3-transition": "^2.0.0",
    "dc": "^4.2.5",
    "jquery": "^3.4.1",
    "leaflet": "^1.5.1",
    "yup": "^0.32.9"
  }
